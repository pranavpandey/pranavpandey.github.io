{
    "entrypoint": "index.html",
    "shell": "src/pp-app.html",
    "fragments": [
        "src/pages/pp-home.html",
        "src/pages/pp-about.html",
        "src/pages/pp-skills.html",
        "src/pages/pp-projects.html",
        "src/pages/pp-work.html",
        "src/pages/pp-contact.html",
        "src/pages/pp-404.html"
    ],
    "sources": [
        "src/**/*",
        "images/**/*",
        "assets/**/*",
        "data/**/*",
        "bower.json",
        "robots.txt"
    ],
    "extraDependencies": [
        "manifest.json",
        "bower_components/webcomponentsjs/*.js"
    ],
    "lint": {
        "rules": [
            "polymer-2-hybrid"
        ]
    },
    "builds": [{
        "preset": "es5-bundled",
        "addServiceWorker": true,
        "bundle": true,
        "js": {
            "minify": true
        },
        "css": {
            "minify": true
        },
        "html": {
            "minify": true
        }
    }, {
        "preset": "es6-bundled",
        "addServiceWorker": true,
        "bundle": true,
        "js": {
            "minify": true
        },
        "css": {
            "minify": true
        },
        "html": {
            "minify": true
        }
    }, {
        "preset": "es6-unbundled"
    }]
}
